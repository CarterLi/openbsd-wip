 $as_echo "$ac_cv_prog_gnu_m4_gnu" >&6; }
-  if test "$ac_cv_prog_gnu_m4_gnu" = yes; then
-    M4_GNU=--gnu
-  else
-    M4_GNU=
-  fi
+  M4_GNU=-g
 
