 	#-Werror=implicit-function-declaration
 endif
 
-ifeq ($(debug), 1)
-	cflags += -g -O0 -fno-inline
-else ifneq ($(noflags), 1)
-	cflags += -g -O2
-endif
-
 ifeq ($(os), windows)
 	cppflags += -DWINDOWS -DWIN32
 	cppflags += -D$(use_sdl)
